{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES2022",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": [
      "eevi/eevi-is",
      "node",
      "electron"
    ],
    "paths": {
      "@app/compat-i18n": [
        "./i18n/index.ts"
      ]
    }
  }
}
