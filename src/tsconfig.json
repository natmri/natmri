{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES2022",
    "allowJs": true,
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "types": [
      "eevi/eevi-is"
    ],
    "paths": {
      "natmri/*": [
        "./natmri/*"
      ],
      "typings/*": [
        "./typings/*"
      ]
    }
  },
  "exclude": [
    "./natmri/entry/electron-browser"
  ]
}
