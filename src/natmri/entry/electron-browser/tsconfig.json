{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "types": [],
    "paths": {
      "#preload/common": [
        "../../parts/preload/common.ts"
      ],
      "#preload/test1": [
        "../../parts/preload/test1.ts"
      ]
    }
  },
  "exclude": [
    "tests",
    "vite.config.ts"
  ]
}
