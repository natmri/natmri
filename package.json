{
  "name": "@natmri/repo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.14.0",
  "description": "Multi platform wallpaper-engine based on electron",
  "author": "starknt",
  "license": "MIT",
  "keywords": [
    "electron"
  ],
  "main": "./out/natmri/background.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "nr clean && electron-vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "dev": "nr clean && electron-vite --sourcemap",
    "preview": "electron-vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "nr build && esno ./scripts/publish.ts",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install",
    "test": "vitest",
    "clean": "esno scripts/clean-cli.ts",
    "release": "bumpp package.json app/package.json --commit --push --tag"
  },
  "dependencies": {
    "@natmri/platform": "^0.0.12",
    "sqlite3": "^5.1.6",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@antfu/ni": "^0.21.12",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.10.8",
    "@types/source-map-support": "^0.5.10",
    "@types/yauzl": "^2.10.3",
    "@unocss/eslint-config": "^0.58.3",
    "bumpp": "^9.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.2",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "pnpm": "8.6.2",
    "solid-js": "^1.8.10",
    "source-map-support": "^0.5.21",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.1.3",
    "yaml": "^2.3.4"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
