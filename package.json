{
  "name": "@natmri/repo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.14.3",
  "description": "Multi platform wallpaper-engine based on electron",
  "author": "starknt",
  "license": "MIT",
  "keywords": [
    "electron"
  ],
  "main": "./out/natmri/background.mjs",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "nr clean && electron-vite build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "dev": "nr clean && electron-vite -w --sourcemap",
    "preview": "electron-vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "nr build && esno ./scripts/publish.ts",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky",
    "test": "vitest",
    "clean": "esno scripts/clean-cli.ts",
    "release": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "@natmri/platform": "^0.0.12",
    "graceful-fs": "^4.2.11",
    "sqlite3": "^5.1.7",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@antfu/ni": "^0.21.12",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/graceful-fs": "^4.1.9",
    "@types/node": "^20.11.7",
    "@types/source-map-support": "^0.5.10",
    "@types/yauzl": "^2.10.3",
    "@unocss/eslint-config": "^0.58.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vueuse/core": "^10.7.2",
    "bumpp": "^9.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "pnpm": "8.6.2",
    "source-map-support": "^0.5.21",
    "taze": "^0.13.1",
    "terser": "^5.27.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.4",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue": "^3.4.15"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
